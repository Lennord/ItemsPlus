function ItemsPlusNoPermission(s: sender):
	load "plugins/ItemsPlus/config.yml"
	yaml value "language" from "config" = "English":
		send "&4| &8x &cYou have not permission to use this command." to {_s}

function ItemsPlusNoCommandConsole(s: sender):
	load "plugins/ItemsPlus/config.yml"
	yaml value "language" from "config" = "English":
		send "&4| &8x &cYou must be a player to use this command." to {_s}

function ItemsPlusHelp(s: sender, typehelp: text):
	load "plugins/ItemsPlus/config.yml"
	yaml value "language" from "config" = "English":
		{_typehelp} = "ItemsPlusCMD":
			send "" to {_s}
			send "&8| &8x &6Help:" to {_s}
			send "" to {_s}
			send "&8| &f/itemsplus &binfo &8x &7Seen informations of this skript." to {_s}
			send "&8| &f/itemsplus &breload &8x &7Reload this skript." to {_s}
			send "&8| &f/itemsplus &bgive &8x &7Give item type." to {_s}
			send "" to {_s}

function ItemsPlusOpenMenu(s: sender, typemenu: text):
	load "plugins/ItemsPlus/config.yml"
	yaml value "language" from "config" = "English":
		{_typemenu} = "OpenMenuMain":
			set metadata tag "ItemsPlusOpenMenuMain" of {_s} to chest inventory with 6 rows named "Give item type"
			yaml value "items.Hammers3x3" from "config" = true:
				set slot 0 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to iron pickaxe named "&fHammers &7(&c3x3&7)" with lore "", "&7These hammers can undermine 3 out of 3 blocks.", "" and "&cClick &6for open menu of give all hammer type." with nbt "{CustomModelData:1}"
			else:
				set slot 0 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to barrier named "&cUnavailable"
			yaml value "items.Hammers5x5" from "config" = true:
				set slot 1 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to iron pickaxe named "&fHammers &7(&c5x5&7)" with lore "", "&7These hammers can undermine 5 out of 5 blocks.", "" and "&cClick &6for open menu of give all hammer type." with nbt "{CustomModelData:1}"
			else:
				set slot 1 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to barrier named "&cUnavailable"
			yaml value "items.UnclaimFinder" from "config" = true:
				set slot 2 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to book named "&fUnclaim Finder" with lore "", "&7This item allows you to find in percentage the", "&7number of block type concerned by this item.", "" and "&cClick &6for open menu of give all unclaim finder type." with nbt "{CustomModelData:1}"
			else:
				set slot 2 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to barrier named "&cUnavailable"
			yaml value "items.MegaBoneMeal" from "config" = true:
				set slot 3 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to bone meal named "&fMega Bone meal" with lore "", "&7This item allows you to grow seeds up to a maximum", "&7of one hit.", "" and "&cClick &6for open menu of give all mega bone meal." with nbt "{CustomModelData:1}"
			else:
				set slot 3 of metadata tag "ItemsPlusOpenMenuMain" of {_s} to barrier named "&cUnavailable"
			yaml value "items.Trash" from "config" = true:
