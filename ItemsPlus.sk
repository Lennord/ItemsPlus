on load:
	yaml file "plugins/ItemsPlus/config.yml" doesn't exist:
		load "plugins/ItemsPlus/config.yml"
		set yaml value "language" from "config" to "English"
		set yaml value "items.Hammers3x3" from "config" to true
		set yaml value "items.Hammers5x5" from "config" to true
		set yaml value "items.UnclaimFinder" from "config" to true
		set yaml value "items.MegaBoneMeal" from "config" to true
		set yaml value "items.Trash" from "config" to true
		set yaml value "items.Katana" from "config" to true
		save yaml "config"
		stop
	else:
		load "plugins/ItemsPlus/config.yml"
		yaml value "language" from "config" != "English":
			set yaml value "language" from "config" to "English"
		save yaml "config"
	unload yaml "config"
	send "&7[&bItemsPlus&7] &8x &2Enabled" to console

