command /itemsplus [<text>] [<text>]:
	aliases: itemsplus:itemsplus
	description: &fItemsPlus command /itemsplus.
	trigger:
		arg 1 isn't set:
			ItemsPlusHelp(sender, "ItemsPlusCMD")
		else:
			arg 1 != "info" or "reload" or "give":
				ItemsPlusHelp(sender, "ItemsPlusCMD")
			arg 1 = "info":
				load "plugins/ItemsPlus/config.yml"
				yaml value "language" from "config" = "English":
					send "" to sender
					send "&8| &8x &6Help:" to sender
					send "" to sender
					send "&8| &fName of skript: &cItemsplus (Skript)" to sender
					send "&8| &fAuthor of skript: &cAlexisl315 (Alias: Lennord)" to sender
					send "&8| &fDescription of skript: &cThis skript allows you to add new items to the game." to sender
					send "&8| &fVersion of skript: &c2.0.4" to sender
					send "&8| &fPlugin requirements: &cSkript&7, &cSkBee &7and &cSkript-YAML" to sender
					send "&8| &fAll languages of skript: &cEnglish" to sender
					send "" to sender
			arg 1 = "reload":
				make console execute command "/sk reload ItemsPlus"
				load "plugins/ItemsPlus/config.yml"
				yaml value "language" from "config" = "English":
					send "&7You have been reloaded this skript." to sender
			arg 1 = "give":
				sender = console:
					ItemsPlusHelp(console, "ItemsPlusCMD")
				sender = player:
					ItemsPlusOpenMenu(player, "OpenMenuMain")

on command:
	sender = player:
		command = "itemsplus" or "itemsplus:itemsplus":
			player doesn't have permission "itemsplus.itemsplus" or "itemsplus.*":
				cancel event
				ItemsPlusNoPermission(player)
