command /itemsplus [<text>] [<text>]:
	executable by: player and console
	aliases: itemsplus:itemsplus
	description: &fItemsPlus command /itemsplus !
	trigger:
		if sender = player:
			if arg 1 isn't set:
				ItemsPlusItemsplusHelp(player)
			else:
				if arg 1 != "reload" or "info" or "give":
					ItemsPlusItemsplusHelp(player)
				if arg 1 = "reload":
					ItemsPlusItemsplusCmdReload(player)
				if arg 1 = "info":
					ItemsPlusItemsplusCmdInfo(player)
				if arg 1 = "give":
					ItemsPlusItemsplusCmdGiveOpenMenu(player)
		if sender = console:
			if arg 1 isn't set:
				ItemsPlusItemsplusHelp(console)
			else:
				if arg 1 != "reload" or "info":
					ItemsPlusItemsplusHelp(console)
				if arg 1 = "reload":
					ItemsPlusItemsplusCmdReload(console)
				if arg 1 = "info":
					ItemsPlusItemsplusCmdInfo(console)

on command "itemsplus" or "itemsplus:itemsplus":
	if sender = player:
		if player doesn't have permission "itemsplus.*" or "itemsplus.itemsplus":
			cancel event
			send "&4| &cYou do not have permission to use this command !" to player
			stop

on inventory click:
	event-inventory = anvil inventory:
		if index of event-slot = 2:
			if event-slot = wooden pickaxe with nbt "{CustomModelData:1}":
				cancel event
				stop
			if event-slot = stone pickaxe with nbt "{CustomModelData:1}":
				cancel event
				stop
			if event-slot = iron pickaxe with nbt "{CustomModelData:1}":
				cancel event
				stop
			if event-slot = golden pickaxe with nbt "{CustomModelData:1}":
				cancel event
				stop
			if event-slot = diamond pickaxe with nbt "{CustomModelData:1}":
				cancel event
				stop
			if event-slot = netherite pickaxe with nbt "{CustomModelData:1}":
				cancel event
				stop