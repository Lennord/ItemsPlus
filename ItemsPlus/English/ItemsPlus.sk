function ItemsPlusItemsplusCmdReload(s: sender):
	if {_s} has permission "itemsplus.*" or "itemsplus.itemsplus.reload" or "itemsplus.itemsplus.*":
		make console execute command "/sk reload ItemsPlus/"
		send "&7[&cItemsPlus&7] &6You have restarted the plugin." to {_s}
		stop
	else:
		send "&4| &8x &cYou do not have permission to use this command." to {_s}
		stop

function ItemsPlusItemsplusCmdInfo(s: sender):
	if {_s} has permission "itemsplus.*" or "itemsplus.itemsplus.info" or "itemsplus.itemsplus.*":
		send "" to {_s}
		send "&c| &6Informations:&f%nl%%nl%&8| &7Name of plugin&6: &cItemsPlus &7(&cSkript&7)&f%nl%&8| &7Description of plugin&6: &cItemsPlus is a skript plugin that allows you to add many new items that have never been seen before in vanilla Minecraft.&f%nl%&8| &7Creator of plugin&6: &cAlexisl315 &7(Aliases: &cLennord&7)&f%nl%&8| &7Version of plugin&6: &c1.0.0 PRE-RELEASE 1&f%nl%&8| &7Support version&6: &c1.18.2&f%nl%&8| &7Plugins required&6: &cSkript&7/&cSkBee &7and &cSkungee &7(for &cBungeecord&7)" to {_s}
		stop
	else:
		send "&4| &8x &cYou do not have permission to use this command." to {_s}
		stop

function ItemsPlusItemsplusHelp(s: sender):
	send "" to {_s}
	send "&6| &8x &6Help:" to {_s}
	send "" to {_s}
	send "&6| &f/itemsplus &breload &8x &7Reload the skript." to {_s}
	send "&6| &f/itemsplus &binfo &8x &7Info to the skript." to {_s}
	send "&6| &f/itemsplus &bgive &8x &7Open menu for give item type." to {_s}
	stop

