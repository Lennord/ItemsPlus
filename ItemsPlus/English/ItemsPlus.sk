function ItemsPlusItemsplusCmdReload(s: sender):
	if {_s} has permission "itemsplus.*" or "itemsplus.itemsplus.reload" or "itemsplus.itemsplus.*":
		make console execute command "/sk reload ItemsPlus/"
		send "&7[&cItemsPlus&7] &6You have restarted the plugin !" to {_s}
		stop
	else:
		send "&4| &cYou do not have permission to use this command !" to {_s}
		stop

function ItemsPlusItemsplusCmdInfo(s: sender):
	if {_s} has permission "itemsplus.*" or "itemsplus.itemsplus.info" or "itemsplus.itemsplus.*":
		send "" to {_s}
		send "&c| &6Informations:&f%nl%%nl%&8| &7Name of plugin&6: &cItemsPlus &7(&cSkript&7)&f%nl%&8| &7Creator of plugin&6: &cAlexisl315 &7(Aliases: &cLennord&7)&f%nl%&8| &7Version of plugin&6: &c1.0.0-SNAPSHOT 2022.2a&f%nl%&8| &7Support version&6: &c1.18.1&f%nl%&8| &7Plugins required&6: &cSkript &7and &cSkungee &7(for &cBungeecord&7)" to {_s}
		stop
	else:
		send "&4| &cYou do not have permission to use this command !" to {_s}
		stop

function ItemsPlusItemsplusHelp(s: sender):
	send "" to {_s}
	send "&c| &6Help:" to {_s}
	send "" to {_s}
	send "&7| &f/itemsplus &breload &6| &7Reload the skript !" to {_s}
	send "&7| &f/itemsplus &binfo &6| &7Info to the skript !" to {_s}
	send "&7| &f/itemsplus &bgive &6| &7Open menu for give item type !" to {_s}
	stop

function ItemsPlusItemsplusCmdGiveOpenMenu(s: sender):
	if {_s} has permission "itemsplus.*" or "itemsplus.give.openmenu" or "itemsplus.give.*":
		set metadata tag "ItemPlusGiveMenu" of {_s} to chest inventory with 6 rows named "&6» &eGive new item"
		set slot 0 of metadata tag "ItemPlusGiveMenu" of {_s} to iron pickaxe named "&fHammer &7(&c3x3&7)" with lore "", "&7These hammers can undermine 3 out of 3 blocks !", "" and "&cClick &6for open menu of give all hammer type..." with nbt "{CustomModelData:1}"
